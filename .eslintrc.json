{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard", "prettier", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-unused-vars": "warn"
  }
}
